---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
<title type="text">{{ site.title | xml_escape }}</title>
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>{{site.baseurl | prepend: site.url }}/</id>
<link rel="alternate" type="text/html" href="{{ site.baseurl | prepend: site.url }}" />
<link rel="self" type="application/atom+xml" href="{{ "/atom.xml" | prepend: site.baseurl | prepend: site.url }}" />
<generator uri="https://github.com/mojombo/jekyll">Jekyll</generator>
<author>
  <name>{{ site.author.name }}</name>
  <uri>{{ site.baseurl | prepend: site.url }}/</uri>
</author>

{% for post in site.posts limit:20 %}
<entry>
  <title type="html"><![CDATA[{{ post.title | cdata_escape }}]]></title>
  <link>{{ post.url | prepend: site.baseurl | prepend: site.url }}</link>
  <id>{{ post.id | prepend: site.baseurl | prepend: site.url }}</id>
  {% if post.modified %}<updated>{{ post.modified | to_xmlschema }}T00:00:00-00:00</updated>
  <published>{{ post.date | date_to_xmlschema }}</published>
  {% else %}<published>{{ post.date | date_to_xmlschema }}</published>
  <updated>{{ post.date | date_to_xmlschema }}</updated>{% endif %}
  <author>
    <name>{{ site.author.name }}</name>
    <uri>{{ site.baseurl | prepend: site.url }}</uri>
  </author>
  <content type="html">{{ post.content | xml_escape }}
  </content>
</entry>
{% endfor %}
</feed>
